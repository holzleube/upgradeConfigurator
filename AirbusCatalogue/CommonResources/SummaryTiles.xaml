    										<DiscreteDoubleKeyFrame KeyTime="0:0:3.47" Value="0"/>
    									</DoubleAnimationUsingKeyFrames>
    									<DoubleAnimationUsingKeyFrames BeginTime="0" Storyboard.TargetProperty="Angle" Storyboard.TargetName="E1R">
